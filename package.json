{
	"name": "copilot",
	"version": "1.0.0",
	"private": true,
	"overrides": {
		"secure-ls": {
			"crypto-js": "^4.2.0"
		}
	},
	"scripts": {
		"dev": "vite --host 0.0.0.0",
		"dev:debug": "DEBUG=vite:* vite",
		"build": "run-p type-check build-only",
		"build-only": "vite build",
		"build-analyzer": "vite-bundle-visualizer -t sunburst",
		"preview": "vite build && echo '' && vite preview --port 4173 --host",
		"preview-only": "vite preview --port 4173 --host",
		"test:unit": "vitest --environment jsdom",
		"test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
		"test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"tailwind-config-viewer": "tailwind-config-viewer -o",
		"design-tokens": "node scripts/tokens-tool.js",
		"start-server-old": "cd backend && uvicorn copilot:app --reload --port=5000",
		"start-server": "cd backend && /opt/venv/bin/python copilot.py",
		"start-vue": "vite --host 0.0.0.0",
		"start": "concurrently \"npm run start-server\" \"npm run start-vue\"",
		"libs-check": "taze",
		"libs-reload": "rm -rf node_modules package-lock.json && npm install",
		"open:swagger": "open http://127.0.0.1:5000/docs#/",
		"open:redoc": "open http://127.0.0.1:5000/redoc"
	},
	"dependencies": {
		"@ajoelp/json-to-formdata": "^1.5.0",
		"@f3ve/vue-markdown-it": "^0.2.0",
		"@fontsource/jetbrains-mono": "^5.0.18",
		"@fontsource/lexend": "^5.0.18",
		"@fontsource/public-sans": "^5.0.16",
		"@popperjs/core": "^2.11.8",
		"@vueuse/components": "^10.7.2",
		"@vueuse/core": "^10.7.2",
		"apexcharts": "^3.45.2",
		"bytes": "^3.1.2",
		"colord": "^2.9.3",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.10",
		"detect-touch-device": "^1.1.6",
		"echarts": "^5.4.3",
		"jose": "^5.2.0",
		"js-md5": "^0.8.3",
		"lodash": "^4.17.21",
		"markdown-it-highlightjs": "^4.0.1",
		"mitt": "^3.0.1",
		"naive-ui": "^2.37.3",
		"password-validator": "^5.3.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persistedstate": "^3.2.1",
		"secure-ls": "^1.2.6",
		"validator": "^13.11.0",
		"vue": "^3.4.15",
		"vue-advanced-cropper": "^2.8.8",
		"vue-highlight-words": "^3.0.1",
		"vue-i18n": "^9.9.0",
		"vue-router": "^4.2.5",
		"vue-sjv": "^0.0.6",
		"vue3-apexcharts": "^1.4.4",
		"vue3-marquee": "^4.1.0"
	},
	"devDependencies": {
		"@clack/prompts": "^0.7.0",
		"@iconify/vue": "^4.1.1",
		"@rushstack/eslint-patch": "^1.7.2",
		"@tsconfig/node18": "^18.2.2",
		"@types/bytes": "^3.1.4",
		"@types/fs-extra": "^11.0.4",
		"@types/highlight.js": "^10.1.0",
		"@types/inquirer": "^9.0.7",
		"@types/jsdom": "^21.1.6",
		"@types/lodash": "^4.14.202",
		"@types/markdown-it": "^13.0.7",
		"@types/markdown-it-highlightjs": "^3.3.4",
		"@types/node": "^20.11.7",
		"@types/validator": "^13.11.8",
		"@vitejs/plugin-vue": "^5.0.3",
		"@vitejs/plugin-vue-jsx": "^3.1.0",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/test-utils": "^2.4.4",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^10.4.17",
		"cypress": "^13.6.3",
		"eslint": "^8.56.0",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-vue": "^9.20.1",
		"fs-extra": "^11.2.0",
		"jsdom": "^24.0.0",
		"json5": "^2.2.3",
		"npm-run-all": "^4.1.5",
		"picocolors": "^1.0.0",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"sass": "^1.70.0",
		"start-server-and-test": "^2.0.3",
		"tailwind-config-viewer": "^1.7.3",
		"tailwindcss": "^3.4.1",
		"taze": "^0.13.1",
		"ts-node": "^10.9.2",
		"typescript": "~5.3.3",
		"unplugin-vue-components": "^0.26.0",
		"vite": "^5.0.12",
		"vite-bundle-analyzer": "^0.7.0",
		"vite-bundle-visualizer": "^1.0.0",
		"vite-svg-loader": "^5.1.0",
		"vitest": "^1.2.2",
		"vue-tsc": "^1.8.27"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
